{
  "background": {
    "persistent": true,
    "scripts": ["dist/background.bundle.js"]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "MacCtrl+E"
      }
    }
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["dist/content.bundle.js"]
  }],
  "description": "",
  "icons": {},
  "manifest_version": 2,
  "name": "<%= project_name %>",
  "options_ui": {
    "page": "dist/options.html",
    "chrome_style": true
  },
  "permissions": [],
  "version": "0.0.0",
  "web_accessible_resources": [
    "dist/iframe.html"
  ]
}
